<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New RosterServer</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @rosterserver.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Username: </td>
                <td class="itemValue"><input type="text" name="rosterserver[username]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Company name: </td>
                <td class="itemValue"><input type="text" name="rosterserver[company_name]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Address: </td>
                <td class="itemValue"><input type="text" name="rosterserver[address]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Phone number: </td>
                <td class="itemValue"><input type="text" name="rosterserver[phone_number]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
